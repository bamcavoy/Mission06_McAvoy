@{
}

@model List<Movie>

<table class="table table-bordered">

    <thead class="fw-bold">
        <tr>
            <td>Title</td>
            <td>Year</td>
            <td>Director</td>
            <td>Category</td>
            <td>Rating</td>
            <td>Edited</td>
            <td>Lent to</td>
            <td>Copied to Plex</td>
            <td>Notes</td>
        </tr>
    </thead>

    @foreach (var m in Model)
    {
        <tbody>
            <tr>
                <td>@m.Title</td>
                <td>@m.Year</td>
                <td>@m.Director</td>
                <td>category</td>
                <td>@m.Rating</td>
                <td>@m.Edited</td>
                <td>@m.LentTo</td>
                <td>@m.CopiedToPlex</td>
                <td>@m.Notes</td>
            </tr>
        </tbody>
    }


</table>